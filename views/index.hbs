<div class="container">
    <h1>{{title}}</h1>
    <ul class="fires">
        {{#each fires}}
            <li>
                <h3><a href="{{this.link}}">{{this.title}}</a></h3>
                <div class="description">
                    {{this.description}}
                </div>
            </li>
        {{/each}}
    </ul>
</div>
